public String userAutentication(String id, String pwd) {
		try {
			System.out.println("id: "+id+" pwd: "+pwd);
			if(id.equals("nombre") && pwd.equals("123")){
				System.out.println("Autenticaci√≥n correcta");
				return "Success";
			}
			else {
				return "Error credentials";
			}
		}
		catch (Exception e) {
			e.printStackTrace();
		}
		return "Failed";
	}
